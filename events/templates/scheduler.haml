    .form-group.repeat-end.hide{"aria-hidden": "true"}
      %label.col-sm-2.control-label.scheduler-label End
      .col-sm-10.row
        .col-sm-3.form-group
          .btn-group.selectlist.end-options.pull-left{"data-resize": "auto"}
            %button.btn.btn-default.dropdown-toggle{"data-toggle": "dropdown", "type": "button"}
              %span.selected-label Never
              %span.caret
              %span.sr-only Never
            %ul.dropdown-menu{"role": "menu"}
              %li{"data-value": "never"}
                %a{"href": "#"} Never
              %li{"data-value": "after"}
                %a{"href": "#"} After
              %li{"data-value": "date"}
                %a{"href": "#"} On date
            %input.hidden.hidden-field{"aria-hidden": "true", "name": "EndSelectlist", "readonly": "readonly", "type": "text"}
        .col-sm-6.form-group.end-option-panel.end-after-panel.pull-left.hide{"aria-hidden": "true"}
          .spinbox.digits-3.end-after
            %label#MyEndAfter.sr-only End After
            %input.form-control.input-mini.spinbox-input{"aria-labelledby": "MyEndAfter", "type": "text"}
              .spinbox-buttons.btn-group.btn-group-vertical
                %button.btn.btn-default.spinbox-up.btn-xs{"type": "button"}
                  %span.glyphicon.glyphicon-chevron-up
                  %span.sr-only Increase
                %button.btn.btn-default.spinbox-down.btn-xs{"type": "button"}
                  %span.glyphicon.glyphicon-chevron-down
                  %span.sr-only Decrease
          .inline-form-text.end-after-text occurance(s)
