#myScheduler.scheduler.row{data-initialize: "scheduler"}
  .start-datetime
    .form-group.col-md-6{class: "{% if form.date.errors %}has-error{% elif form.data and form.date %}has-success{% endif %}"}
      %label.control-label.scheduler-label{for: "myStartDate"}
        Date (optional)
      .datepicker.start-date
        .input-group
          %input#myStartDate.form-control{"type": "text", name: "date"}/
          .input-group-btn
            %a.btn.btn-default.dropdown-toggle{"data-toggle": "dropdown", "type": "button"}
              %span.glyphicon.glyphicon-calendar
              %span.sr-only Toggle Calendar
            .dropdown-menu.dropdown-menu-right.datepicker-calendar-wrapper{"role": "menu"}
              .datepicker-calendar
                .datepicker-calendar-header
                  %a.prev
                    %span.glyphicon.glyphicon-chevron-left
                    %span.sr-only Previous Month
                  %a.next
                    %span.glyphicon.glyphicon-chevron-right
                    %span.sr-only Next Month
                  %a.title
                    %span.month
                      %span{"data-month": "0"} January
                      %span{"data-month": "1"} February
                      %span{"data-month": "2"} March
                      %span{"data-month": "3"} April
                      %span{"data-month": "4"} May
                      %span{"data-month": "5"} June
                      %span{"data-month": "6"} July
                      %span{"data-month": "7"} August
                      %span{"data-month": "8"} September
                      %span{"data-month": "9"} October
                      %span{"data-month": "10"} November
                      %span{"data-month": "11"} December
                    %span.year
                %table.datepicker-calendar-days
                  %thead
                    %tr
                      %th Su
                      %th Mo
                      %th Tu
                      %th We
                      %th Th
                      %th Fr
                      %th Sa
                  %tbody
                .datepicker-calendar-footer
                  %a.datepicker-today Today
              .datepicker-wheels{"aria-hidden": "true"}
                .datepicker-wheels-month
                  %h2.header Month
                  %ul
                    %li{"data-month": "0"}
                      %button{type: "button"} Jan
                    %li{"data-month": "1"}
                      %button{type: "button"} Feb
                    %li{"data-month": "2"}
                      %button{type: "button"} Mar
                    %li{"data-month": "3"}
                      %button{type: "button"} Apr
                    %li{"data-month": "4"}
                      %button{type: "button"} May
                    %li{"data-month": "5"}
                      %button{type: "button"} Jun
                    %li{"data-month": "6"}
                      %button{type: "button"} Jul
                    %li{"data-month": "7"}
                      %button{type: "button"} Aug
                    %li{"data-month": "8"}
                      %button{type: "button"} Sep
                    %li{"data-month": "9"}
                      %button{type: "button"} Oct
                    %li{"data-month": "10"}
                      %button{type: "button"} Nov
                    %li{"data-month": "11"}
                      %button{type: "button"} Dec
                .datepicker-wheels-year
                  %h2.header Year
                  %ul
                .datepicker-wheels-footer.clearfix
                  %a.btn.datepicker-wheels-back
                    %span.glyphicon.glyphicon-arrow-left
                    %span.sr-only Return to Calendar
                  %a.btn.datepicker-wheels-select{"type": "button"}
                    Select
                    %span.sr-only Month and Year
